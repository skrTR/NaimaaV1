{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Access to your photo library is required to attach photos to scan qr code or product images upload."
        }
      ]
    ],
    "name": "Naimaa",
    "slug": "Naimaa-naimaa",
    "version": "0.1.9",
    "orientation": "portrait",
    "icon": "./assets/ioslogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.naimaa",
      "buildNumber": "0.1.9",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Access to your photo library is required to attach photos to scan qr code or product images upload.",
        "NSCameraUsageDescription": "Naimaa requires access to your camera to goods photo to upload and goods scanning qr code"
      }
    },

    "android": {
      "versionCode": 19,
      "package": "mn.naimaa",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/androidlogo.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
